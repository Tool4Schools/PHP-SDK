{
  "name": "tools4schools/php-sdk",
  "authors": [
    {
      "name": "Timothy Seebus",
      "email": "timothyseebus@tools4schools.org"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "league/oauth2-client": "^2.3",
    "symfony/options-resolver": "^6.0",
    "psr/http-client-implementation": "^1.0",
    "illuminate/support": "^9.3"
  },

  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "php-http/mock-client": "^1.0",
    "guzzlehttp/guzzle": "^7.0"
  },

  "autoload": {
    "psr-4": {
      "Tools4Schools\\SDK\\": "./src/Tools4Schools/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Tools4Schools\\Tests\\SDK\\": "./tests/"
    }
  },

  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
